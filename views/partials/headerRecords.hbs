<div class="header-title">
    <ul class="flex-container list header-list">
        <li class="header-item item-one">
            <h4>LSU Events and Attendance Tracking Website</h4>
            <div id="errorResponse" class="flex-container user-prompt slide-in"></div>
            <div id="successResponse" class="flex-container user-prompt slide-in"></div>
        </li>
        <li class="header-item item-two">
            <h5 id="adminData"> {{adminData.organization}} Officer | {{adminData.first_name}} {{adminData.last_name}}
            </h5>
        </li>
        <li class="header-item item-three">
            <img src="../img/User_cicrle_duotone.png" alt="Profile Image" />
        </li>
        <li class="header-item item-four">
            <a href="/help"><img src="../img/Question_fill.png" alt="Help and Resources" /></a>
        </li>
    </ul>
</div>

<div id="idNumberContainer" data-idNumber="{{idNumber}}" style="display: none;"></div>

{{> officerPermission}}

<input type="hidden" id="isAdminURL" value="{{isAdminURL}}" />
<input type="hidden" id="isEditURL" value="{{isEditURL}}" />
<input type="hidden" id="isAdminPageURL" value="{{isAdminPageURL}}" />
<input type="hidden" id="isRecordPage" value="{{isRecordPage}}" />